{
  "name": "financing-aip",
  "version": "1.0.0",
  "scripts": {
    "mock": "cross-env API_BASE_URL=http://localhost:82 MOCK=1 vue-cli-service serve",
    "serve": "cross-env API_BASE_URL=http://localhost:82 vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,less,css}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,less,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "build:all": "npm run build",
    "ui": "vue ui",
    "viewlint": "cross-env NODE_ENV=production eslint -f html > ./lint-output/lint.html --ext .ts,.js,.vue src/",
    "output:eslint": "cross-env NODE_ENV=production eslint -f json > ./lint-output/eslint.json --ext .ts,.js,.vue src/",
    "output:stylelint": "cross-env NODE_ENV=production stylelint -f json \"src/**/*.{vue,less,css}\" > ./lint-output/stylelint.json",
    "output": "run-p output:*"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "@zl/dialog": "^0.1.2",
    "@zl/loading": "^0.1.1",
    "@zl/toast": "^0.1.4",
    "core-js": "^2.6.5",
    "fastclick": "^1.0.6",
    "vue-awesome-swiper": "3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "3.7.x",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@zl/eslint-plugin-zllint": "0.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "5.2.x",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "8.1.x",
    "markdownlint-cli": "0.15.x",
    "npm-run-all": "4.1.x",
    "postcss-px-to-viewport": "^1.1.1",
    "stylelint": "10.0.x",
    "stylelint-config-css-modules": "1.4.x",
    "stylelint-config-prettier": "5.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.3.x",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "iOS >= 9",
    "Android >= 4.4"
  ],
  "author": "K0240029\n",
  "template": "1.0.6",
  "description": ""
}
